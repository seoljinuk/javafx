<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="330.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itgroup.controller.LoginTestController">
    <children>
        <Label styleClass="labelClass"  layoutX="50.0" layoutY="50.0" text="아이디 : hong, 비번 : 1234">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label id="labelId" layoutX="50.0" layoutY="100.0" text="아이디">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label styleClass="labelClass" layoutX="50.0" layoutY="150.0" text="비번">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <Label fx:id="fxmlResult" layoutX="50.0" layoutY="200.0" text="속성 바인딩">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="fxmlId" layoutX="120.0" layoutY="95.0">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </TextField>
        <PasswordField fx:id="fxmlPassword"  layoutX="120.0" layoutY="145.0">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </PasswordField>
      <Button fx:id="btnOk" layoutX="80.0" layoutY="250.0" mnemonicParsing="false" text="로그인">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
        <Button fx:id="btnCancel" layoutX="180.0" layoutY="250.0" mnemonicParsing="false" text="초기화">
            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
